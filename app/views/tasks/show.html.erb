<%= stylesheet_link_tag "jquery.inlineeditor" %>
<%= stylesheet_link_tag "shared/modal" %>
<%= stylesheet_link_tag "tasks/show" %>
<%= stylesheet_link_tag "tasks/show_new" %>
<%= javascript_include_tag "plugins/jquery.simplemodal-1.2.2.js"%>
<%= javascript_include_tag "tasks/show" %>
<%
  @title = h(@task.title)
  @hide_layout_title = true
%>

<% with_locals :task => @task do |t| %>
  <%= t.render :partial => "task_dialog_data" %>
  <%= t.render :partial => "task_main" %>
  <%= t.render :partial => "task_news" %>
  <%= t.render :partial => "confirm_completion_dialog" %>
  <%= t.render :partial => "pre_submit_dialog" %>
  <%= t.render :partial => "post_submit_dialog" %>
<% end %>
